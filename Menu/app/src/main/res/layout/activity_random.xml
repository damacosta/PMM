<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/LinearLayout1"
    tools:context="com.example.alemen.menu.random"
    android:orientation="vertical" >

    <Button
        android:onClick="launchDrawShapes1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Pulsa para hacer un dibujo aleatorio" />

    <Button
        android:onClick="launchDrawShapes2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Crea dibujos aleatorios al clickar" />

</LinearLayout>
